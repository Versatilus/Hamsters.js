{
  "name": "hamsters.js",
  "author": "Austin K Smith",
  "description": "100% Vanilla Javascript Multithreading & Parallel Execution Library",
  "homepage": "http://www.hamsters.io/",
  "version": "4.2.2",
  "main": "build/hamsters.js",
  "scripts": {
    "build": "babel-node ./scripts/cleanBuildDirectory && webpack --config ./hamsters.webpack.config.js",
    "dev": "webpack-dev-server --content-base src/index --hot --host 0.0.0.0 --config ./webpack.config.js",
    "watch": "webpack --progress --colors --watch --config ./webpack.config.js",
    "test": "karma start --reporters html"
  },
  "license": "Artistic-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/austinksmith/Hamsters.js.git"
  },
  "keywords": [
    "multithreading",
    "concurrency",
    "performance",
    "parallelism",
    "compute",
    "workers",
    "threads",
    "threading",
    "mpi",
    "web workers",
    "parallel",
    "task runner",
    "thread pool"
  ],
  "bugs": {
    "url": "https://github.com/austinksmith/Hamsters.js/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "eslint": "^3.6.0",
    "eslint-config-airbnb": "^11.2.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fs-extra": "^0.30.0",
    "html-webpack-plugin": "^2.22.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "1.0.0",
    "karma-htmlfile-reporter": "~0.3",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^1.8.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
  "directories": {
    "test": "spec"
  },
  "dependencies": {}
